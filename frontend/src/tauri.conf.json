{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "cession-app-frontend",
  "version": "0.1.0",
  "identifier": "com.electro.cessionapp",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "externalBin": [
      "backend.jar"
    ]
  },
  "app": {
    "windows": [
      {
        "title": "cession-app-frontend",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icono.ico"
    ]
  }
} 